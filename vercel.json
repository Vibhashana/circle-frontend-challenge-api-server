{
    "version": 2,
    "builds": [
        {
            "src": "dist/src/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/books(.*)",
            "dest": "dist/src/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "dist/src/index.js"
        }
    ]
}
